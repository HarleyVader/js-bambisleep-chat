            color: var(--button-color);
            margin-left: 0.3rem;
        }

        p {
            color: var(--primary-alt);
            margin-left: 2rem;
        }

        strong {
            color: var(--secondary-alt);
            margin-left: 1rem;
        }

        a {
            color: var(--tertiary-alt);
            text-decoration: underline;
        }

        a:hover {
            color: var(--secondary-alt);
        }

        #ascii-container {
            height: 220px;
            margin: 20px auto;
            max-width: 800px;
            border: 2px solid var(--tertiary-color);
            border-radius: 8px;
            background-color: rgba(0, 0, 0, 0.7);
        }

